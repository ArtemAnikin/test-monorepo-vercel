{
  "name": "monorepo-vercel",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "build": "nx run-many -t build",
    "dev:next-web": "nx run next-web:dev",
    "dev:next-docs": "nx run next-docs:dev",
    "dev:vite-dashboard": "nx run vite-dashboard:dev",
    "build:next-web": "nx run next-web:build",
    "build:next-docs": "nx run next-docs:build",
    "build:vite-dashboard": "nx run vite-dashboard:build",
    "lint:next-web": "nx run next-web:lint",
    "lint:next-docs": "nx run next-docs:lint",
    "lint:vite-dashboard": "nx run vite-dashboard:lint",
    "start:next-web": "nx run next-web:start",
    "start:next-docs": "nx run next-docs:start",
    "preview:vite-dashboard": "nx run vite-dashboard:preview",
    "affected:build": "nx affected -t build",
    "affected:lint": "nx affected -t lint"
  },
  "devDependencies": {
    "nx": "^22.5.1"
  }
}
